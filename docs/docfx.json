{
  "metadata": [
    {
      "src": [
        {
          "files": [ "src/Core/Core.csproj" ],
          "src": "../"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "separatePages",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/*"
        ]
      },
      {
        "files": [
          "assets/*"
        ],
        "src": "../"
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Tyne Docs",
      "_appLogoPath": "assets/logo-docs-header.svg",
      "_appFaviconPath": "assets/logo-small.svg",
      "_appFooter": "Built locally",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "markdownEngineProperties": {
      "alerts": {
        "TODO": "alert alert-secondary"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
