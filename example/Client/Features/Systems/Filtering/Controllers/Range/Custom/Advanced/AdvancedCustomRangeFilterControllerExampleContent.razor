<SimpleFilterContext TRequest="AdvancedCustomRangeControllerExampleRequest">
    <FilterValues>
        <TyneFilterValue For="m => m.TargetOrbitalAltitudePerigee" Default="413_000" />
        <TyneFilterValue For="m => m.TargetOrbitalAltitudeApogee" Default="422_000" />
    </FilterValues>
    <ChildContent Context="ConfiguredRequest">
        <AltitudeRangePickerController ForMin="m => m.TargetOrbitalAltitudePerigee"
                                       ForMax="m => m.TargetOrbitalAltitudeApogee" />
    </ChildContent>
</SimpleFilterContext>
