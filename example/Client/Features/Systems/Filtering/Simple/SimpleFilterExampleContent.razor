<SimpleFilterContext TRequest="SimpleExampleRequest">
    <FilterValues>
        <TyneFilterValue For="m => m.MissionName" />
    </FilterValues>
    <ChildContent Context="ConfiguredRequest">
        <TyneStringFilterController For="m => m.MissionName" Label="Mission name" />
        <MudText Class="mt-3" Typo="Typo.h6">
            Mission name: @ConfiguredRequest.MissionName
        </MudText>
    </ChildContent>
</SimpleFilterContext>
