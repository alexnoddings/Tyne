@namespace Tyne.Blazor.Tables.Columns
@typeparam TRequest
@typeparam TResponse
@typeparam TValue
@inherits Tyne.Blazor.Filtering.Controllers.TyneMultiSelectBoxFilterController<TRequest, TValue>

<TyneColumnPopoverHeader TResponse="TResponse"
                         IsActive="Value?.Count > 0"
